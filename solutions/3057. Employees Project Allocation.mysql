WITH CTE AS(
    SELECT
    E.EMPLOYEE_ID,P.PROJECT_ID,E.NAME,P.WORKLOAD AS PROJECT_WORKLOAD,
    AVG(P.WORKLOAD) OVER(PARTITION BY E.TEAM) AS AVG_WL
    FROM 
    EMPLOYEES E
    INNER JOIN PROJECT P
    ON E.EMPLOYEE_ID=P.EMPLOYEE_ID)
SELECT EMPLOYEE_ID,PROJECT_ID,NAME AS EMPLOYEE_NAME,PROJECT_WORKLOAD
FROM CTE
WHERE PROJECT_WORKLOAD > AVG_WL
ORDER BY 1,2
