WITH CTE AS(
    SELECT
    CASE
        WHEN INCOME<20000 THEN 'Low Salary'
        WHEN INCOME>50000 THEN 'High Salary'
        ELSE 'Average Salary'
    END AS CATEGORY,ACCOUNT_ID
    FROM ACCOUNTS
),
CTE_C AS(
SELECT 'Low Salary' AS CATEGORY
UNION
SELECT 'High Salary' AS CATEGORY
UNION
SELECT 'Average Salary' AS CATEGORY)
SELECT 
CTE_C.CATEGORY,COUNT(CTE.ACCOUNT_ID) AS ACCOUNTS_COUNT FROM
CTE RIGHT JOIN CTE_C
ON CTE.CATEGORY = CTE_C.CATEGORY
GROUP BY CTE.CATEGORY
